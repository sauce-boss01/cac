local script = G2L["e"];
	local player = game.Players.LocalPlayer
	local favoriteItemsFolder = player:FindFirstChild("FavoriteItems")
	local output = script.Parent.Parent.Parent.output
	
	script.Parent.MouseButton1Up:Connect(function()
		if favoriteItemsFolder then
			local favoriteItems = favoriteItemsFolder:GetChildren()
	
			if #favoriteItems > 0 then
				for _, item in pairs(favoriteItems) do
					item:Destroy()
				end
				output.Text = "successfully deleted all favorite items"
